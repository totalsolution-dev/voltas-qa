            // Reconsiled OpenGraph tags for Facebook and Twitter
            <meta property="og:title" content="European Travel Destinations">
            <meta property="og:description" content="Offering tour packages for individuals or groups.">
            <meta property="og:image" content="http://euro-travel-example.com/thumbnail.jpg">
                // Image constraints: follow Facebook's recommendation of a minimum dimension of 1200x630 pixels and 
                an aspect ratio of 1.91:1, but adhere to Twitter's file size requirement of less than 1MB.
            <meta property="og:url" content="http://euro-travel-example.com/index.htm">
            <meta name="twitter:card" content="summary_large_image">
            <meta name="twitter:image:alt" content="Alt text for image">
                // This provides an alternative image description for those who are visually impaired.



